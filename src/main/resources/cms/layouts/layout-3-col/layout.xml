<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<layout-component>
  <display-name>3 columns</display-name>
  <config>
    <input name="columnConfig" type="SingleSelector">
      <label>Column configuration</label>
      <immutable>false</immutable>
      <indexed>false</indexed>
      <occurrences minimum="1" maximum="1"/>
      <config>
        <options>
          <option>
            <label>33-33-33</label>
            <value>33-33-33</value>
          </option>
          <option>
            <label>50-25-25</label>
            <value>50-25-25</value>
          </option>
          <option>
            <label>25-50-25</label>
            <value>25-50-25</value>
          </option>
          <option>
            <label>25-25-50</label>
            <value>25-25-50</value>
          </option>
        </options>
        <selector-type>DROPDOWN</selector-type>
      </config>
    </input>
  </config>
  <regions>
    <region name="1"/>
    <region name="2"/>
    <region name="3"/>
  </regions>
</layout-component>